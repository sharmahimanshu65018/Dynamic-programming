using namespace std;
void fast()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	//freopen("input.txt", "r", stdin);
	//freopen("output.txt", "w", stdout);

}
int fib(int n,vector<int>&v)
{

	if (n == 0)
	{
		return 0;
	}
	else if (n == 1)
	{
		return 1;
	}
	if (v[n] != 0)
	{
		return v[n];
	}
	v[n] = fib((n - 1),v) + fib((n - 2),v);
	return v[n];
}
int main()
{
	fast();
	int n;
	cin >> n;
	vector<int>v(n+1, 0);
	
	cout << fib(n, v);
	
}
